{
    "version":2,
    "builds":[
        {
            "src":"@app/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/.(.*)",
            "dest":"@app/main.ts",
            "methods":[
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}